{
    "name": "ot-ng2-tools",
    "version": "0.0.1",
    "description": "Angular 2 tools",
    "main": "index.js",
    "scripts": {
        "watch": "tsc -p src -w",
        "build": "rm -rf lib && tsc",
        "typings": "./node_modules/typings/dist/bin.js",
        "webdriver-update": "webdriver-manager update",
        "webdriver-start": "webdriver-manager start",
        "postinstall": "npm run webdriver-update && typings install",
        "e2e": "protractor",
        "e2e-live": "protractor --elementExplorer",
        "test": "karma start",
        "test-watch": "karma start --no-single-run --auto-watch",
        "ci": "npm run e2e && npm run test"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/opstalent/ot-ng2-tools.git"
    },
    "keywords": [
        "Angular2",
        "Module"
    ],
    "author": "bosek.mateusz@gmail.com",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/opstalent/ot-ng2-tools/issues"
    },
    "homepage": "https://github.com/opstalent/ot-ng2-tools#readme",
    "dependencies": {
        "@angular/compiler": "2.1.0",
        "@angular/common": "2.1.0",
        "@angular/core": "2.1.0",
        "@angular/platform-browser": "2.1.0",
        "@angular/platform-browser-dynamic": "2.1.0",
        "core-js": "^2.4.1",
        "rxjs": "5.0.0-beta.12",
        "zone.js": "^0.6.23",
        "ot-ng2-oauth": "git+https://github.com/opstalent/ot-ng2-oauth.git"
    },
    "devDependencies": {
        "typescript": "^2.0.2",
        "typings": "^1.3.2",
        "webpack": "^1.13.0",
        "node-sass": "^3.4.2",
        "ts-loader": "^0.8.1",
        "awesome-typescript-loader": "^2.2.4",
        "angular2-template-loader": "^0.4.0",
        "html-loader": "^0.4.3",
        "null-loader": "^0.1.1",
        "raw-loader": "^0.5.1",
        "json-loader": "^0.5.3",
        "postcss-loader": "^0.9.1",
        "sass-loader": "^4.0.0",
        "karma": "^1.2.0",
        "karma-webpack": "^1.8.0",
        "karma-jasmine": "^1.0.2",
        "karma-coverage": "^1.0.0",
        "karma-mocha-reporter": "^2.0.3",
        "karma-remap-istanbul": "0.1.1",
        "karma-phantomjs-launcher": "^1.0.2",
        "karma-sourcemap-loader": "^0.3.7",
        "istanbul-instrumenter-loader": "^0.2.0",
        "remap-istanbul": "^0.6.4",
        "jasmine-core": "^2.4.1",
        "jasmine-spec-reporter": "^2.4.0",
        "phantomjs-prebuilt": "^2.1.7",
        "protractor": "^3.1.1"
    }
}